{
  "name": "study-cafe",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "start": "yarn serve",
    "build": "yarn build-ts",
    "preserve": "yarn build",
    "serve": "node dist/server.js",
    "watch-node": "sleep 5 && nodemon dist/server.js",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"yarn watch-ts\" \"yarn watch-node\"",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "generate:resource:document": "hygen generate document-resource",
    "postgenerate:resource:document": "yarn format",
    "add:property:document": "hygen property add-to-document",
    "postadd:property:document": "yarn format",
    "add:property:object": "hygen property add-to-object",
    "postadd:property:object": "npm run format",
    "add:roles": "node ./dist/data/import-dev-data.js --import",
    "drop:roles": "node ./dist/data/import-dev-data.js --delete",
    "seed:roles": "yarn drop:roles && yarn add:roles"
  },
  "dependencies": {
    "@types/bcrypt-nodejs": "0.0.30",
    "@types/compression": "0.0.36",
    "@types/cors": "^2.8.4",
    "@types/express": "^4.16.1",
    "@types/lodash": "^4.17.0",
    "@types/mongoose": "^5.3.17",
    "@types/node": "^11.9.4",
    "@types/passport": "^1.0.0",
    "@types/passport-jwt": "^3.0.1",
    "@types/passport-local": "^1.0.33",
    "@types/uuid": "^3.4.4",
    "bcrypt": "^5.1.1",
    "bcrypt-nodejs": "0.0.3",
    "compression": "^1.7.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.17.3",
    "helmet": "^7.1.0",
    "html-to-text": "^9.0.5",
    "hygen": "^6.2.11",
    "lodash": "^4.17.21",
    "mailersend": "^2.3.0",
    "mongoose": "^8.4.5",
    "nodemon": "^2.0.20",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "typescript": "^5.5.3",
    "uuid": "^3.3.2",
    "winston": "^3.12.0",
    "winston-daily-rotate-file": "^5.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/swagger-jsdoc": "^6.0.4",
    "concurrently": "^7.6.0",
    "tslint": "^5.12.1"
  },
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/bahaa-alden/study-cafe.git"
  }
}
